<item id="form">



	<input type="input" required="true" maxlength="40" key="geo">
		<label>Please enter the country you currently work on.</label>
	</input>

	<input type="select" required="true" maxlength="40" key="edu">
		<label>Please select your educational level.</label>
		<option name="PhD">phd</option>
		<option name="PhD Student">phdstu</option>
		<option name="Master">msc</option>
		<option name="Master Student">mscstu</option>
		<option name="Undergraduate">und</option>
		<option name="Undergraduate Student">undstud</option>
		<option name="Other">other</option>
	</input>

	<input type="multiCheckbox" required="true" maxlength="40" key="occupation">
		<label>What is your current job? (You can select multiple options)</label>
		<option name="Open-Source Developer ">opedev</option>
		<option name="Industrial Developer">inddev</option>
		<option name="Academic Researcher">acares</option>
		<option name="Industrial Researcher">indres</option>
		<option name="Other">other</option>
	</input>

